{
    "name": "mvishal/adminlte",
    "description": "AdminLTE template applied to Laravel Scaffolding",
    "license": "MIT",
    "type": "package",
    "keywords": ["bootstrap", "theme", "admin", "template", "administrator","admin-lte","adminlte", "laravel","mvishal"],
    "homepage": "https://github.com/mvishu405/adminlte",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.*"
    },
    "author": {
        "name": "Mvishal",
        "email": "mvishu405@gmail.com",
        "homepage": ""
    },
    "autoload": {
        "psr-4": {
            "Mvishal\\Adminlte\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvishal\\Adminlte\\AdminlteServiceProvider"
            ],
            "aliases": {
              "Adminlte": "Mvishal\\Adminlte\\Facade\\Adminlte"
            }
        }
    },
    "scripts": {
      "post-root-package-install": [
        "php artisan vendor:publish --tag=adminlte --force",
        "php artisan adminlte:info"
      ]
    },
    "minimum-stability": "dev"
}